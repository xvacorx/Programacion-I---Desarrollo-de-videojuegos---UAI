//5. Dado como dato el importe de venta de una factura, calcular el valor correspondiente
//al precio final, sum√°ndole el IVA (21% del valor de venta). Mostrar los tres valores.

Console.WriteLine("Importe de importe de venta: ");
float Importe = float.Parse(Console.ReadLine());

double IVA = Importe * 0.21;

double Total = IVA + Importe;

Console.WriteLine("El importe es de: " + Importe);
Console.WriteLine("El IVA es de: " + IVA);
Console.WriteLine("El total es de: " + Total);
