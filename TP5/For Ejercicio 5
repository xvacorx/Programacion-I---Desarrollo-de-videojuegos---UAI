//5. Calcular e informar la potencia de un número, ingresando por teclado la base y el exponente.

using System.Transactions;

Console.WriteLine("Ingrese número base");
int Numero = int.Parse(Console.ReadLine());

Console.WriteLine("Ingrese exponente");
int Exponente = int.Parse(Console.ReadLine());

int Potencia = 1;

for (int i = 0; i < Exponente; i++)
{
    Potencia *= Numero;
}

Console.WriteLine("La base " + Numero + " elevado a la potencia " + Exponente + " da un resultado de: " + Potencia);
