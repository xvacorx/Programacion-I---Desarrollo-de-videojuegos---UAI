//2. Ingresando una nota final de cursada (valor con decimales) informar si la condici칩n es: Promovido, Regular o Libre.
//Libre: menor a 4
//Regular: mayor o igual a 4 y menor a 6
//Promovido: mayor o igual a 6.

Console.WriteLine("Ingrese nota del alumno");
float Nota = float.Parse(Console.ReadLine());

if (Nota >= 6) { Console.WriteLine("El alumno est치 promovido"); }
else if (Nota < 4) { Console.WriteLine("El alumno qued칩 libre"); }
else { Console.WriteLine("El alumno est치 regular"); }
